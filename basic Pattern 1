#include<stdio.h>
int main()
{
  int i,n,j;
  printf("Enter number of rows :");
  scanf("%d", &n);
  printf("\n");
  for(i=0; i <= n; i++){
    for(j = 0; j < i; j++){
       int k = j;
       if(k > 25)
            k = k - 26;
    printf("%c",'A'+k);
    }
      printf("\n");
  }
}
/*
sample output: 
A
AB
ABC
ABCD
*/
